<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:npui="urn:import:com.adamantium.company.gwtp.client.application.details.ui.tabs"
             xmlns:npui2="urn:import:com.adamantium.company.gwtp.client.application.details.ui.linkmenu">

    <ui:with field="res" type="com.adamantium.company.gwtp.client.resources.AppResources"/>

    <g:HTMLPanel>

        <header>
            <div class="{res.style.container}">
                <h1>GWTP Basic Spring Example</h1>
                <ul>
                    <li>
                        <a href="/gwtpcompany.html">[ Project Home ]</a>
                    </li>
                    <li>
                        <a href="https://github.com/ArcBees/GWTP-Samples/tree/master/gwtp-samples/gwtp-sample-basic-spring">
                            [ Source ]
                        </a>
                    </li>
                </ul>
            </div>
        </header>

        <div class="{res.style.container}" style="margin-top:15px">
            <div style="margin-bottom:10px">
                <g:Button ui:field="goToList">Close</g:Button>
            </div>
            <npui:RoundTabPanel ui:field="tabPanel"/>
            <npui2:LinkMenu ui:field="linkMenu"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
