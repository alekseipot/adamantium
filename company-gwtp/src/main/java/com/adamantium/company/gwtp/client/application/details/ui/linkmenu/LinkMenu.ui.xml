<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type="com.adamantium.company.gwtp.client.place.NameTokens" field="nameTokens"/>

    <ui:with field="resources" type="com.adamantium.company.gwtp.client.resources.AppResources"/>

    <g:HTMLPanel tag="footer">
        <g:InlineHyperlink targetHistoryToken="{nameTokens.getNameDetails}">Company</g:InlineHyperlink>
        |
        <g:InlineHyperlink targetHistoryToken="{nameTokens.getAddressDetails}">Contacts</g:InlineHyperlink>
    </g:HTMLPanel>
</ui:UiBinder>
